@{
    var data = ViewData["mydata"] as IEnumerable<Item>;

}


<table class="table table-dark table-sm">
    <thead>
    <td>Username</td>
    <td>Phone Number</td>
    <td>Item Description</td>
    <td>Loction Description</td>
    <td>Found Floor</td>
    </thead>
    <tbody>
        @foreach (Item x in data)
        {
            <tr>
                <td>@x.Uname </td>
                <td>@x.UserContact</td>
                <td>@x.IteamDesc</td>
                <td>@x.LostLoctionDesc</td>
                <td>@x.FloorDetails</td>
            </tr>
        }
    </tbody>


</table>
